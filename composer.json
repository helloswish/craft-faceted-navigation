{
    "name": "swishdigital/faceted-navigation",
    "description": "Provides faceted navigation of entries, using categories, which allows site users to narrow the list of entries they see by applying multiple filters (think Amazon or eBay left sidebar).",
    "type": "craft-plugin",
    "version": "5.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "navigation",
        "faceted navigation",
        "categories",
        "category",
        "filter",
        "filters",
        "filtering",
        "sidebar"
    ],
    "support": {
        "docs": "https://github.com/helloswish/craft-faceted-navigation/blob/master/README.md",
        "issues": "https://github.com/helloswish/craft-faceted-navigation/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Swish Digital",
            "homepage": "https://swishdigital.co"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0-alpha.1|^5.0.x-dev|^5.0.0"
    },
    "autoload": {
        "psr-4": {
          "swishdigital\\facetednavigation\\": "src/"
        }
    },
    "extra": {
        "name": "Faceted Navigation",
        "handle": "faceted-navigation",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/helloswish/craft-faceted-navigation/master/CHANGELOG.md",
        "components": {
            "navigation": "swishdigital\\facetednavigation\\services\\Navigation"
        },
        "class": "swishdigital\\facetednavigation\\FacetedNavigation"
    }
}
